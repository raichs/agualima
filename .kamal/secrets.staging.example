# Staging Secrets Example

# Docker Hub Token (create at https://hub.docker.com/settings/security)
KAMAL_REGISTRY_PASSWORD="your_docker_hub_token_here"

# Laravel App Key (generate with: php artisan key:generate --show)
APP_KEY="base64:your_laravel_app_key_here"

# Database credentials
DB_CONNECTION="mysql"
DB_HOST="your_db_host"
DB_PORT="your_db_port"
DB_DATABASE="your_db_database"
DB_USERNAME="your_db_username"
DB_PASSWORD="your_secure_password_here"

# MySQL Root Password
MYSQL_ROOT_PASSWORD="your_mysql_root_password_here"
MYSQL_DATABASE="your_db_database"
MYSQL_USER="your_db_username"
MYSQL_PASSWORD="your_secure_password_here"
